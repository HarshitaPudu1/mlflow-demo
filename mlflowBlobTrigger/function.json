{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "myblob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "demo-data",
      "connection": "blobstorageaccountsource_STORAGE"
    },
    {
      "name": "$return",
      "type": "blob",
      "direction": "out",
      "path": "demo-data/output-data",
      "connection": "blobstorageaccountdestination_STORAGE"
    }
  ]
}
